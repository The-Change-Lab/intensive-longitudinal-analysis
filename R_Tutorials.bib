	issn = {1554-3528},
	journaltitle = {Behavior Research Methods},
	keywords = {Humans, Software, Depression, Data Interpretation, Statistical, Algorithms, Classification and regression trees, Computer Simulation, Linear Models, Meta-Analysis as Topic, Mixed-effects models, Model-based recursive partitioning, Treatment-subgroup interactions},
	number = {5},
	pages = {2016--2034},
	pmid = {29071652},
	shortjournal = {Behav Res Methods},
	title = {Detecting treatment-subgroup interactions in clustered data with generalized linear mixed-effects model trees},
	volume = {50},
	bdsk-url-1 = {https://doi.org/10.3758/s13428-017-0971-x}}

