	keywords = {longitudinal panel},
	note = {Publisher: Annual Reviews Type: Journal Article},
	number = {Volume 57, 2006},
	pages = {505--528},
	title = {Analysis of {Longitudinal} {Data}: {The} {Integration} of {Theoretical} {Model}, {Temporal} {Design}, and {Statistical} {Model}},
	url = {https://www.annualreviews.org/content/journals/10.1146/annurev.psych.57.102904.190146},
